{
  "version": 2,
  "builds": [
    {
      "src": "dist/index.js",
      "use": "@vercel/node"
    }
  ],
  "functions": {
    "api/*.js": {
      "maxDuration": 10,
      "maxPayload": 100
    },
    "dist/index.js": {
      "maxDuration": 10,
      "maxMemory": 1024,
      "maxPayload": 100
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/dist/index.js"
    }
  ],
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "buildCommand": "npm run build"
}
